{
  "name": "rush",
  "version": "0.1.0",
  "description": "Rush theme for Magento 2",
  "license": "(OSL-3.0 OR AFL-3.0)",
  "author": "Magento Commerce",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/magento-research/pwa-module-theme"
  },
  "scripts": {
    "build": "webpack --progress --color --env production",
    "jest": "run-s jest:theme jest:lib",
    "jest:theme": "JEST_PROJECT=theme jest src",
    "jest:lib": "JEST_PROJECT=node-scripts jest lib",
    "lint": "eslint lib && (echo; echo Not linting src directory, change ASAP!; echo;)",
    "prepare": "npm-merge-driver install",
    "prettier": "prettier --write './**/*.js'",
    "prettier:check": "prettier-check './**/*.js'",
    "start": "webpack-dev-server --progress --color --env development",
    "start:debug": "node --inspect-brk ./node_modules/.bin/webpack-dev-server --progress --color --env development",
    "test": "run-p jest prettier:check lint",
    "test:dev": "jest --watch"
  },
  "dependencies": {
    "@magento/peregrine": "^0.1.1-alpha",
    "babel-runtime": "^6.26.0",
    "dotenv": "^4.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "@magento/eslint-config": "^1.0.0",
    "@magento/workbox-webpack-plugin": "^3.0.0-alpha-magento.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.28.7",
    "death": "^1.1.0",
    "eol": "^0.9.1",
    "eslint": "^4.14.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "express": "^4.16.2",
    "jest": "^21.2.1",
    "make-fetch-happen": "^2.6.0",
    "node-forge": "^0.7.1",
    "npm-merge-driver": "^2.3.5",
    "npm-run-all": "^4.1.2",
    "pkg-dir": "^2.0.0",
    "prettier": "^1.7.4",
    "prettier-check": "^2.0.0",
    "selfsigned": "^1.10.2",
    "style-loader": "^0.19.0",
    "tmp": "0.0.33",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.11.0",
    "write-file-webpack-plugin": "^4.2.0"
  }
}
